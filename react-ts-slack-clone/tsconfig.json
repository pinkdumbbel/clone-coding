{
    "compilerOptions": {
      "esModuleInterop": true,
      "sourceMap": true,
      "lib": ["ES2020", "DOM"],
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "Node",
      "target": "es5",
      "strict": true,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {
        "@hooks/*": ["src/hooks/*"],
        "@components/*": ["src/components/*"],
        "@src/*": ["src/*"],
        "@pages/*": ["src/pages/*"],
        "@utils/*": ["src/utils/*"],
        "@typings/*": ["src/types/*"]
      }
    }
  }